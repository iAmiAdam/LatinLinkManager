<% provide(:title, 'New Project') %>
<div class="col-xs-12 col-sm-12 col-md-4 col-lg-4 col-md-offset-4 col-lg-offset-4">
<h1 class="center"> Create Project </h1>
<div class="well">
	<%= form_for(@project) do |f| %>
	<%= render 'shared/error_messages', object: f.object %>
	<div class="form-group">
		<%= f.label :translator %>
		<%= f.input :translator, :collection => get_translators_name, :include_blank => false, input_html => {:rel => 'autocomplete'} class: "form-control"  %>
	</div>
	
	<%= f.submit "Create New Project", class: "btn btn-large btn-primary form-control" %>
	<% end %>
</div>
</div>