<% provide(:title, 'New Translator') %>
<div class="col-xs-12 col-sm-12 col-md-4 col-lg-4 col-md-offset-4 col-lg-offset-4">
<h1 class="center"> Create New Translator </h1>
<div class="well">
	<%= form_for(@translator) do |f| %>
	<%= render 'shared/error_messages', object: f.object %>
	<div class="form-group">
		<%= f.label :name %>
		<%= f.text_field :name, class: "form-control"  %>
	</div>

	<div class="form-group">
		<%= f.label :email %>
		<%= f.text_field :email, class: "form-control"  %>
	</div>

	<div class="form-group">
		<%= f.label :rate %>
		<%= f.text_field :rate, class: "form-control"  %>
	<div>

	<%= f.submit "Create New Translator", class: "btn btn-large btn-primary form-control" %>
	<% end %>
</div>
</div>